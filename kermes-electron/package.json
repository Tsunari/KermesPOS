{
  "name": "kermes-pos",
  "version": "1.9.0",
  "description": "POS system with USB printer support",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "docs": "jsdoc -c docs/jsdoc.json",
    "docs:type": "typedoc --options docs/typedoc.json"
  },
  "keywords": [],
  "author": "Tunahan K",
  "license": "AGPL-3.0",
  "devDependencies": {
    "electron": "36.2.0",
    "electron-builder": "26.0.12",
    "jsdoc": "^4.0.4",
    "typedoc": "^0.28.4"
  },
  "dependencies": {
    "electron-updater": "^6.6.2",
    "node-wmi": "0.0.5"
  },
  "build": {
    "appId": "com.kermes.pos",
    "productName": "Kermes POS",
    "win": {
      "target": "nsis",
      "icon": "assets/Logo-light-m.ico"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "Tsunari",
        "repo": "KermesPOS"
      }
    ],
    "files": [
      "main.js",
      "preload.js",
      "print_receipt_win.py",
      {
        "from": "python-3.13.3-embed-amd64",
        "to": "python-3.13.3-embed-amd64",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "util",
        "to": "util",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "build",
        "to": "build",
        "filter": [
          "**/*"
        ]
      }
    ],
    "asarUnpack": [
      "python-3.13.3-embed-amd64/**",
      "print_receipt_win.py"
    ]
  }
}
